{
    "version": 2,
    "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }],
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.js"
        }
    ]
}